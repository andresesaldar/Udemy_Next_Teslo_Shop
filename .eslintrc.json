{
  "extends": [
    "next/core-web-vitals",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "@typescript-eslint/explicit-function-return-type": "error",
    "@typescript-eslint/member-ordering": "error",
    "no-console": "error",
    "sort-imports": "error",
    "sort-keys": "error",
    "sort-vars": "error"
  }
}
